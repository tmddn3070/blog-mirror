#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/astro@4.11.0_@types+node@20.14.7_lightningcss@1.25.1_sass@1.77.6_stylus@0.63.0_terser@5.31.1_typescript@5.5.2/node_modules/astro/node_modules:/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/astro@4.11.0_@types+node@20.14.7_lightningcss@1.25.1_sass@1.77.6_stylus@0.63.0_terser@5.31.1_typescript@5.5.2/node_modules:/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/astro@4.11.0_@types+node@20.14.7_lightningcss@1.25.1_sass@1.77.6_stylus@0.63.0_terser@5.31.1_typescript@5.5.2/node_modules/astro/node_modules:/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/astro@4.11.0_@types+node@20.14.7_lightningcss@1.25.1_sass@1.77.6_stylus@0.63.0_terser@5.31.1_typescript@5.5.2/node_modules:/Users/smoo/Documents/Coding/smooblog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
